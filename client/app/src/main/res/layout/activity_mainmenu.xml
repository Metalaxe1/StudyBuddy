<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MainMenu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.uncg.studdybuddy.studybuddy.MainMenu">


    <ImageButton
        app:srcCompat="@drawable/profile"
        android:id="@+id/profileButton"
        android:clickable="true"
        android:layout_width="139dp"
        android:layout_height="156dp"
        android:visibility="visible"
        android:layout_margin="0dp"
        tools:layout_editor_absoluteY="19dp"
        tools:layout_editor_absoluteX="26dp" />

    <ImageButton
        app:srcCompat="@drawable/settings"
        android:id="@+id/settingsButton"
        android:layout_width="140dp"
        android:layout_height="156dp"
        android:visibility="visible"
        android:layout_margin="0dp"
        android:clickable="true"
        tools:layout_editor_absoluteX="220dp"
        app:layout_constraintTop_toTopOf="@+id/profileButton" />

    <ImageButton
        app:srcCompat="@drawable/classes"
        android:id="@+id/classesButton"
        android:layout_width="148dp"
        android:layout_height="148dp"
        android:visibility="visible"
        android:layout_margin="0dp"
        android:clickable="true"
        tools:layout_editor_absoluteY="264dp"
        app:layout_constraintRight_toRightOf="@+id/profileButton"
        app:layout_constraintLeft_toLeftOf="@+id/profileButton" />

    <ImageButton
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/imageButton8"
        android:layout_width="141dp"
        android:layout_height="152dp"
        android:visibility="visible"
        android:layout_margin="0dp"
        android:clickable="true"
        app:layout_constraintLeft_toLeftOf="@+id/settingsButton"
        app:layout_constraintRight_toRightOf="@+id/settingsButton"
        app:layout_constraintBottom_toBottomOf="@+id/classesButton" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="302dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="302dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="97dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="97dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        app:layout_constraintGuide_begin="289dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="289dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        app:layout_constraintGuide_begin="97dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="97dp"
        tools:layout_editor_absoluteX="0dp" />

</android.support.constraint.ConstraintLayout>
